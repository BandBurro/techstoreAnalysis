CREATE TABLE test_users (
    id INT PRIMARY KEY AUTO_INCREMENT,
    first_name VARCHAR(50),
    last_name VARCHAR(50),
    UNIQUE (first_name, last_name)
);
INSERT INTO test_users (first_name, last_name) VALUES ('Ana', 'Silva');
INSERT INTO test_users (first_name, last_name) VALUES ('Ana', 'Silva');  -- Should fail!
INSERT INTO test_users (first_name, last_name) VALUES ('Ana', 'Souza');  -- Should work
